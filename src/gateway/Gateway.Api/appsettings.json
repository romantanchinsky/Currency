{
  "ReverseProxy": {
    "Routes": {
      "user": {
        "ClusterId": "userCluster",
        "Match": { "Path": "/api/auth/{**catch-all}" }
      },
      "finance": {
        "ClusterId": "financeCluster",
        "Match": { "Path": "/api/currencies/{**catch-all}" }
      }
    },
    "Clusters": {
      "userCluster": {
        "Destinations": {
          "d1": { "Address": "http://user.api:8080/" }
        }
      },
      "financeCluster": {
        "Destinations": {
          "d1": { "Address": "http://finance.api:8080/" }
        }
      }
    }
  }
}
